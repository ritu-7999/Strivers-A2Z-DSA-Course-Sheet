class Solution {
    public String largestOddNumber(String num) {
        int x=num.length()-1;
        String res="";
        while(x>=0){
            int r=(int)num.charAt(x);
            if(r%2==1){
              res=num.substring(0,x+1);
              break;
            }
            if(x==0 && r%2==1){
             res=res+num.charAt(0);
            }
          x--;
         }
        return res;
    }
}
